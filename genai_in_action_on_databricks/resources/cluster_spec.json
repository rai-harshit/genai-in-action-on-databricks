{
    "cluster_name": "GPU_Compute",
    "spark_version": "14.3.x-gpu-ml-scala2.12",
    "aws_attributes": {
        "first_on_demand": 1,
        "availability": "SPOT_WITH_FALLBACK",
        "zone_id": "auto",
        "spot_bid_price_percent": 100,
        "ebs_volume_count": 0
    },
    "node_type_id": "g4dn.4xlarge",
    "driver_node_type_id": "g4dn.2xlarge",
    "autotermination_minutes": 60,
    "enable_elastic_disk": false,
    "single_user_name": "user.name@email.com",
    "enable_local_disk_encryption": false,
    "data_security_mode": "SINGLE_USER",
    "runtime_engine": "STANDARD",
    "effective_spark_version": "14.3.x-gpu-ml-scala2.12",
    "assigned_principal": "user:user.name@email.com",
    "autoscale": {
        "min_workers": 2,
        "max_workers": 2
    },
    "apply_policy_default_values": false
}